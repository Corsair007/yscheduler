package com.yeahmobi.yscheduler.agentframework.exception;

/**
 * @author Leo.Liang
 */
public class TaskSubmitException extends Exception {

    public TaskSubmitException() {
        super();
    }

    public TaskSubmitException(String message) {
        super(message);
    }

    public TaskSubmitException(String message, Throwable cause) {
        super(message, cause);
    }

    public TaskSubmitException(Throwable cause) {
        super(cause);
    }
}
