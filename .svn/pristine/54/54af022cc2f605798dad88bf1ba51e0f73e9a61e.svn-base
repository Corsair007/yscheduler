package com.yeahmobi.yscheduler.model.type;

public enum TaskType {

    SHELL(1), HADOOP(1);

    private int id;

    TaskType(int id) {
        this.id = id;
    }

    public int getId() {
        return this.id;
    }

    public String getName() {
        return this.name();
    }

    public void setId(int id) {
        this.id = id;
    }

    public static TaskType valueOf(int id) {
        switch (id) {
            case 1:
                return SHELL;
        }
        return null;
    }

}
