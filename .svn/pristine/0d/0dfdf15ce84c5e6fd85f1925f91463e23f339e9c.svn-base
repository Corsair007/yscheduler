<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<artifactId>parent</artifactId>
		<groupId>com.yeahmobi.yscheduler</groupId>
		<version>0.4.0</version>
	</parent>
	<artifactId>scheduler</artifactId>
	<packaging>jar</packaging>
	<name>${project.artifactId}</name>
	<description>调度系统</description>

	<dependencies>
		<dependency>
			<artifactId>common</artifactId>
			<groupId>com.yeahmobi.yscheduler</groupId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<artifactId>model</artifactId>
			<groupId>com.yeahmobi.yscheduler</groupId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<artifactId>agent-framework</artifactId>
			<groupId>com.yeahmobi.yscheduler</groupId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.google.guava</groupId>
			<artifactId>guava</artifactId>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-eclipse-plugin</artifactId>
				<version>2.9</version>
				<configuration>
					<downloadSources>true</downloadSources>
					<downloadJavadocs>true</downloadJavadocs>
					<additionalConfig>
						<file>
							<name>.settings/org.eclipse.jdt.ui.prefs</name>
							<location>../.eclipse/org.eclipse.jdt.ui.prefs</location>
						</file>
						<file>
							<name>.settings/org.eclipse.jdt.core.prefs</name>
							<location>../.eclipse/org.eclipse.jdt.core.prefs</location>
						</file>
					</additionalConfig>
				</configuration>
			</plugin>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
					<encoding>UTF-8</encoding>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>
