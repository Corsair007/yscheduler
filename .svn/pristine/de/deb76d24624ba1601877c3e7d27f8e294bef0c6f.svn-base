	<script type="text/javascript"> 
			//从服务器上获取当前时间及时区
			var timeInfo = $.ajax({url:window.contextPath,async: false});
			var timeZone = decodeURIComponent(timeInfo.getResponseHeader("timeZone"));
			var currentDate = new Date(timeInfo.getResponseHeader("systemTime"));
			function run() 	{ 
			currentDate.setSeconds(currentDate.getSeconds() + 5); 
			var time = "";
			var year = currentDate.getFullYear();
			var month = currentDate.getMonth() + 1;
			var day = currentDate.getDate();
			var hour = currentDate.getHours();
			var minute = currentDate.getMinutes();
			var second = currentDate.getSeconds();
			if(hour < 10){
				time += "0" + hour;
			}else{
				time += hour;
			}
			time += ":";
			if(minute < 10){
				time += "0" + minute;
			}else{
				time += minute;
			}
			document.getElementById("systemTime").innerHTML ="<i class='icon-calendar'></i>   "+ timeZone+ "&nbsp;&nbsp;" + year+"年"+month+"月"+day+"日" + time;     
			} 
			run();
			window.setInterval("run();", 1000*5); 
		</script>
