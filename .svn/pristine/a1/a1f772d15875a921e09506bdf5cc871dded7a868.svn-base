<div class="page-header">
	<h1>
		<a href="${rc.contextPath}/workflow/detail/$!{workflow.id}">$!{workflow.name}</a>
		<small> <i class="icon-double-angle-right"></i> 工作流运行详情
		</small>
	</h1>
</div>
<div id="alertMessageDiv" style="text-align: center;"></div>
<div class="col-xs-12">
	<div class="table-responsive">
		<table class="table table-striped table-bordered table-hover"
			id="instanceListTable">
			<thead>
				<tr>
					<th>任务名称</th>
					<th>开始时间</th>
					<th>状态</th>
					<th>操作</th>
				</tr>
			</thead>
			<tbody id="adList">
				#foreach($instance in $list)
				<tr>
					<td>$instance.taskName</td>
					<td>$dateFormatUtils.format($instance.startTime,'yyyy年MM月dd日HH时mm分')</td>
					<td>#if($instance.status.id==1) <span
						class="label label-sm label-info">
							#elseif($instance.status.id==2) <span
							class="label label-sm label-info ">
								#elseif($instance.status.id==3) <span
								class="label label-sm label-success ">
									#elseif($instance.status.id==4) <span
									class="label label-sm label-danger "> #end
										$instance.status.desc</span></td>
					<td><a
						href="${rc.contextPath}/mytask/instance/attempt?instanceId=$instance.id"><span
							class="label label-info">运行情况 </span></a></td>
				</tr>
				#end
			</tbody>
		</table>
	</div>
</div>
#screen("paginator.htm")
